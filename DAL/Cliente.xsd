            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT        Cliente.NIT, Cliente.Nombre + '  ' + Cliente.Apellidos AS Nombre, SUM(Credito.Monto) AS Monto, SUM(Credito.SaldoPendiente) AS SaldoPendiente, SUM(Venta.Total) AS TotalVenta
